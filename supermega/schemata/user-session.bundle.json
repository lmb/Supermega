{
"request": {
    "schema": {
        "type": "object",
        "properties": {
            "a": {"type": "string", "pattern": "us", "required": true},
            "user": {"type": "string", "pattern": "[^@]+@\\w+\\.\\w{2,}", "required": true},
            "uh": {"type": "string", "minLength": 8, "maxLength": 12, "required": true, "pattern": "[\\w-]+"}
        }
    },

    "mapping": {
        "user": "user",
        "hash": "uh"
    }
},
"response": {
    "schema": {
        "type": "object",
        "properties": {
            "csid": {"type": "string", "required": true},
            "privk": {"type": "string", "required": true},
            "k": {"type": "string", "required": true}
        }
    },

    "mapping": {
        "session_id": "csid",
        "private_key": "privk",
        "master_key": "k"
    }
}}